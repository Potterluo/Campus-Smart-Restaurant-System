AA,CAAA,UAAA,EAAA,SAAA,CADA;AACA;AACA,MAAA,KAAA,EAAA,MAFA;AAGA,MAAA,IAAA,EAAA,oCAHA,CAIA;;AAJA,KAAA,CAAA;AAMA,IAAA,MAAA,CAAA,EAAA,CAAA,OAAA,EAAA,YAAA,EA5BA,CA+BA;;AACA,IAAA,GAAA,CAAA,GAAA,CAAA,MAAA,EAhCA,CAmCA;;AACA,QAAA,IAAA,GAAA,EAAA;AAEA,IAAA,IAAA,CAAA,IAAA,CAAA,CAAA,UAAA,EAAA,SAAA,CAAA;AACA,IAAA,IAAA,CAAA,IAAA,CAAA,CAAA,UAAA,EAAA,SAAA,CAAA;AACA,IAAA,IAAA,CAAA,IAAA,CAAA,CAAA,UAAA,EAAA,SAAA,CAAA;AAEA,IAAA,GAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,YAAA;AACA,UAAA,KAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,GAAA,EAAA,IAAA,EAAA,IAAA,CAAA,aAAA,CAAA,SAAA,CAAA,CADA,CAEA;;AACA,MAAA,KAAA,CAAA,MAAA;AACA,KAJA;AAMA,QAAA,QAAA,GAAA,IAAA,IAAA,CAAA,QAAA,CAAA;AACA,MAAA,IAAA,EAAA,IADA;AAEA,MAAA,YAAA,EAAA,CAFA;AAGA,MAAA,YAAA,EAAA,CAHA;AAGA;AACA,MAAA,WAAA,EAAA,OAJA;AAKA,MAAA,WAAA,EAAA,SALA;AAKA;AACA,MAAA,QAAA,EAAA,OANA,CAMA;;AANA,KAAA,CAAA;AAQA,IAAA,GAAA,CAAA,GAAA,CAAA,QAAA,EAxDA,CA2DA;;AACA,IAAA,GAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,YAAA;AACA,UAAA,WAAA,GAAA,IAAA,IAAA,CAAA,WAAA,CAAA;AACA;AACA,QAAA,kBAAA,EAAA,IAFA;AAGA;AACA,QAAA,OAAA,EAAA,KAJA;AAKA;AACA,QAAA,MAAA,EAAA,CAAA,EAAA,EAAA,EAAA,CANA;AAOA;AACA,QAAA,cAAA,EAAA,IARA;AASA;AACA,QAAA,QAAA,EAAA;AAVA,OAAA,CAAA;AAaA,MAAA,WAAA,CAAA,kBAAA,CAAA,UAAA,MAAA,EAAA,MAAA,EAAA;AACA,YAAA,MAAA,IAAA,UAAA,EAAA;AACA,UAAA,UAAA,CAAA,MAAA,CAAA;AACA,SAFA,MAEA;AACA,UAAA,OAAA,CAAA,MAAA,CAAA;AACA;AACA,OANA;;AAQA,eAAA,UAAA,CAAA,IAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;;AAEA,eAAA,OAAA,CAAA,IAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,IAAA;AACA;AACA,KA/BA;AAiCA,GArGA;AAsGA,EAAA,OAAA,EAAA;AAtGA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"container\" style=\"width: 100%; height: calc(100vh - 100px)\"></div>\n\n    <div id=\"info\"></div>\n  </div>\n</template>\n\n<script>\nvar content = [\n  \"<div style='font-size: 14px; color: red; width: 200px; height: 50px'>这是信息窗口</div>\"\n];\n\n\nexport default {\n  name: \"Map\",\n  data() {\n    return {}\n  },\n  created() {\n\n  },\n  mounted() {\n    // 创建地图实例\n    var map = new AMap.Map(\"container\", {\n      zoom: 12,\n      // center: [116.396901,39.919544],\n      resizeEnable: true\n    })\n\n    var infoWindow = new AMap.InfoWindow({\n      anchor: 'top-right',\n      content: content.join(\"<br>\")  //传入 dom 对象，或者 html 字符串\n    });\n\n    var clickHandler = function(e) {\n      console.log('您在[ '+e.lnglat.getLng()+','+e.lnglat.getLat()+' ]的位置点击了地图！');\n\n      infoWindow.open(map, [116.396901,39.919544]);\n    };\n\n    // 绑定事件\n    // map.on('click', clickHandler);\n\n    var marker = new AMap.Marker({\n      position: new AMap.LngLat(116.396901,39.919544),   // 经纬度对象，也可以是经纬度构成的一维数组[116.39, 39.9]\n      title: '北京故宫',\n      icon: '//vdata.amap.com/icons/b18/1/2.png',\n      // content: '<div style=\"font-size: 12px; width: 100px\">这是我自定义的内容</div>'\n    })\n    marker.on('click', clickHandler);\n\n\n    // 将创建的点标记添加到已有的地图实例：\n    map.add(marker);\n\n\n    // path 是驾车导航的起、途径和终点，最多支持16个途经点\n    var path = []\n\n    path.push([116.303843, 39.983412])\n    path.push([116.321354, 39.896436])\n    path.push([116.407012, 39.992093])\n\n    map.plugin('AMap.DragRoute', function () {\n      var route = new AMap.DragRoute(map, path, AMap.DrivingPolicy.LEAST_FEE)\n      // 查询导航路径并开启拖拽导航\n      route.search()\n    })\n\n    var polyLine = new AMap.Polyline({\n      path: path,\n      strokeWeight: 5,\n      borderWeight: 5, // 线条宽度，默认为 1\n      strokeStyle: \"solid\",\n      strokeColor: '#DC143C', // 线条颜色\n      lineJoin: 'round' // 折线拐点连接处样式\n    })\n    map.add(polyLine)\n\n\n    //实例化城市查询类\n    map.plugin('AMap.Geolocation', function() {\n      var geolocation = new AMap.Geolocation({\n        // 是否使用高精度定位，默认：true\n        enableHighAccuracy: true,\n        // 设置定位超时时间，默认：无穷大\n        timeout: 10000,\n        // 定位按钮的停靠位置的偏移量\n        offset: [10, 20],\n        //  定位成功后调整地图视野范围使定位位置及精度范围视野内可见，默认：false\n        zoomToAccuracy: true,\n        //  定位按钮的排放位置,  RB表示右下\n        position: 'RB'\n      })\n\n      geolocation.getCurrentPosition(function(status,result){\n        if(status=='complete'){\n          onComplete(result)\n        }else{\n          onError(result)\n        }\n      });\n\n      function onComplete (data) {\n        // data是具体的定位信息\n        console.log(data)\n      }\n\n      function onError (data) {\n        // 定位出错\n        console.error(data)\n      }\n    })\n\n  },\n  methods: {}\n}\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":"src/views"}]}