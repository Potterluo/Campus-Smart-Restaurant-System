is.request.get(\"/user/username/\" + username).then(res => {\n          // 重新赋值后台的最新User数据\n          this.user = res.data\n          this.$store.state.user= this.user;\n        })\n      }\n    }\n  }\n}\n</script>\n\n"],"sourceRoot":"src/views"}]}